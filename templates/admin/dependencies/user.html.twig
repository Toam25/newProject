Utilisateur inscrit {{ users|length }}
{% for user in users %}
    {% set avatar = "images_default/default_image.jpg"%}

    {% if user.avatar is defined %}
            {% set avatar = user.avatar  %}
    {% endif %}
    <div class="container_user">
            <div class="_container_image_user">
                <img src="{{asset('images/'~avatar)}}" alt="">
            </div> 
             <div class="container_detail_user">
                    <h4 style="color: black;font-weight: bold;">{{ user.name }}&nbsp{{ user.firstName }} </h4> 
                    <span style="font-size: 11px;">Depuis {{ user.createdAt|date('d-m-Y')}}</span>
             </div>
    </div>
{% endfor %}
