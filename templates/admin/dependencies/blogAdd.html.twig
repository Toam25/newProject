{{ form_start(blogForm, {'attr': {'class' : 'add_blog_'}})}}
<div class="row">
    <input type="hidden" class="status" name="status" val="">
    
	<div class="col-md-6">
		{{ form_row(blogForm.title) }}
	</div>
	<div class="col-md-6">
		{{ form_row(blogForm.category) }}
	</div>

	<div class="col-md-12">
            <label>Image</label>
            <input type="file" name="image_blog" class="file form-control">
    </div>

	<div class="col-md-12">

		{{ form_row(blogForm.resume) }}
	</div>

	<div class="col-md-12">

		{{ form_row(blogForm.keywords) }}
	</div>

	<div class="col-md-12">

		{{ form_row(blogForm.metaDescription) }}
	</div>

	<div class="col-md-12">

		{{ form_row(blogForm.link) }}
	</div>

	<div class="col-md-12">

		{{ form_row(blogForm.description) }}
	</div>

</div>
<div class="conbtnSubmit">
	{#<button class="view_blog btn btn-primary">Apper√ßu/Enregister</button>#}
	<button class="save_blog btn btn-success">Publier</button>
</div>

{{ form_end(blogForm) }}
