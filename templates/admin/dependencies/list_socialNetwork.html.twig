<a class="btn btn-success"  href="{{path('social_network_new')}}">Ajouter</a>
<div class="scrol_slide_animation">
      {% for list_socialNetwork in list_socialNetworks %}
 	    <div id=""class="slide_animation">
            <img class="image_slider_to"src="{{ asset('images/'~list_socialNetwork.images )}}">
            <div>
            <p class="_description_link">{{ list_socialNetwork.description}}&nbsp
                    <a href="{{ list_socialNetwork.link}}" target="_blank">{{ list_socialNetwork.nameLink}}</a>
            <p>
            </div>
            <button type="button" val="{{list_socialNetwork.id}}" class="btn btn-danger delete_social_network" style="width: 50%;">Supprimer</button>
            <a href="{{path('social_network_edit',{'id':list_socialNetwork.id })}}" class="btn btn-primary" style="width: 50%;">Editer</a>
        </div>
    {% endfor %}
</div>
