{% for shop in shops %}
        {% set lat = "" %}
        {% set long = ""  %}
        {% if shop.longLat | length > 0 %}
                {% set lat = shop.longLat['lat'] %}
                {% set long = shop.longLat['long']  %}
        {% endif %}
        <div class="list_shop js_market" 
        data-lat = "{{lat}}"
        data-log="{{long}}"
        data-name="{{shop.name}}"
        >
                <h4>{{ shop.name }}</h4>
                <address>{{ shop.address}}</address>
                <a href="{{ path('shop',{'id': shop.id, 'type' : shop.type} )}}">En savoir plus</a>
        </div>
{% endfor %}